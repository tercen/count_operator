{
  "name": "Count",
  "description": "Counts the number of data points present in each cell of the input projection.",
  "tags": [
    "statistics",
    "count"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/count_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Stratifying factor for rows (optional)",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Stratifying factor for columns (optional)",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Observations",
                "type": "numeric",
                "description": "Observations to be counted per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1..n",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}
